$(document).ready(function(){
  $("#1sul").click(function(){
    $("#acesso-1").fadeToggle(500);
    $("form").fadeIn(500);
    $("#acesso-2, #acesso-3, #acesso-4, #acesso-5, #acesso-6, #acesso-7, #acesso-8, #acesso-9, #acesso-10, #acesso-11, #acesso-12").hide();
  });
});

